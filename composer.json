{
    "name": "andresreyesdev/chilexpress-php-sdk",
    "description": "PHP Chilexpress SDK",
    "keywords": [
        "AndresReyesDev",
        "chilexpress-php-sdk"
    ],
    "homepage": "https://github.com/andresreyesdev/chilexpress-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√©s Reyes Galgani",
            "email": "andres@reyes.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "AndresReyesDev\\ChilexpressPhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndresReyesDev\\ChilexpressPhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
